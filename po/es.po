#, sh-format
msgid ""
"Please set the BM_BURNING_MEDIA conf key in $conffile, I assume you want "
"'cdrom'"
msgstr ""
"Por favor, ajuste la opción de configuración BM_BURNING_MEDIA en $conffile; "
"se asume 'cdrom'"

#: actions.sh.ext:100
#, sh-format
msgid ""
"Please set the BM_BURNING_DEVICE conf key in $conffile, I assume you want '/"
"dev/cdrom'"
msgstr ""
"Por favor, ajuste la opción de configuración BM_BURNING_DEVICE en $conffile; "
"se asume '/dev/cdrom'"

#: actions.sh.ext:105
#, sh-format
msgid ""
"Please set the BM_BURNING_METHOD conf key in $conffile, I assume you want "
"'CDRW'"
msgstr ""
"Por favor, ajuste la opción de configuración BM_BURNING_METHOD en $conffile; "
"se asume 'CDRW'"

#: actions.sh.ext:110
#, sh-format
msgid ""
"Please set the BM_BURNING_MAXSIZE conf key in $conffile, I assume you want "
"'650'"
msgstr ""
"Por favor, ajuste la opción de configuración BM_BURNING_MAXSIZE en "
"$conffile; se asume '650'"

